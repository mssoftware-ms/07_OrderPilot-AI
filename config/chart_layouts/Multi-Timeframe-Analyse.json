{
  "name": "Multi-Timeframe-Analyse",
  "description": "Übergeordneter Trend mit 3 Zeitebenen für Pre-Trade Analyse",
  "windows": [
    {
      "symbol": "BTC/USD",
      "timeframe": "1D",
      "period": "3M",
      "monitor": 0,
      "x": 0,
      "y": 0,
      "width": 800,
      "height": 500,
      "indicators": [
        "SMA",
        "RSI"
      ],
      "auto_stream": false
    },
    {
      "symbol": "BTC/USD",
      "timeframe": "1H",
      "period": "2W",
      "monitor": 0,
      "x": 800,
      "y": 0,
      "width": 800,
      "height": 500,
      "indicators": [
        "EMA",
        "MACD"
      ],
      "auto_stream": false
    },
    {
      "symbol": "BTC/USD",
      "timeframe": "5T",
      "period": "2D",
      "monitor": 0,
      "x": 0,
      "y": 500,
      "width": 1600,
      "height": 500,
      "indicators": [
        "BB",
        "RSI"
      ],
      "auto_stream": true
    }
  ],
  "sync_crosshair": true,
  "sync_time_range": false
}