\par def enforce_logical_constraints(df):
\par     """
\par     Korrigiert physikalisch unm\'f6gliche OHLC-Beziehungen.
\par     """
\par     # 1. High < Low Fix (Swap)
\par     swap_mask = df['high'] < df['low']
\par     df.loc[swap_mask, ['high', 'low']] = df.loc[swap_mask, ['low', 'high']].values
\par     
\par     # 2. Negative Preise entfernen
\par     cols = ['open', 'high', 'low', 'close']
\par     for c in cols:
\par         df.loc[df[c] <= 0, c] = np.nan
\par         
\par     return df
